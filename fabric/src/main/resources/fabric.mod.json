    "issues": "https://github.com/henkelmax/better-respawn/issues",
    "sources": "https://github.com/henkelmax/better-respawn",
    "email": "info@modrepo.de"
  },
  "license": "All Rights Reserved",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "de.maxhenkel.betterrespawn.FabricBetterRespawnMod"
    ]
  },
  "mixins": [
    "better_respawn.mixins.json"
  ],
  "depends": {
    "fabricloader": "${fabric_loader_dependency}",
    "minecraft": [
      "1.21",
      "1.21.1"
    ],
    "java": ">=${java_version}"
